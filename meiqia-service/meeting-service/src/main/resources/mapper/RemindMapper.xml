<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.cl.meiqia.mapper.RemindMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.cl.meiqia.entity.Remind">
        <id column="id" property="id" />
        <result column="patient_id" property="patientId" />
        <result column="meeting_id" property="meetingId" />
        <result column="remind_time" property="remindTime" />
        <result column="remind_text" property="remindText" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, patient_id, meeting_id, remind_time, remind_text
    </sql>

</mapper>
